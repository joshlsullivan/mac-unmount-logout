<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.example.unmount</string>
    <key>ProgramArguments</key>
    <array>
        <string>path/to/unmount.sh</string>
    </array>
    <key>StandardOutPath</key>
    <string>/var/log/unmount.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/unmount_error.log</string>
    <key>Debug</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
</dict>
</plist>
